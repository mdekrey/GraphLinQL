<Project>

  <PropertyGroup>
    <BuildRootDir>$(MSBuildThisFileDirectory)</BuildRootDir>
    <ToolsRootDir Condition=" '$(ToolsRootDir)' == '' ">$(MSBuildThisFileDirectory)GraphQlInterfaceGeneration/</ToolsRootDir>
    <RootNamespace>GraphLinqQL</RootNamespace>
    <LangVersion Condition=" '$(LangVersion)' == '' ">8.0</LangVersion>
    <Nullable Condition=" '$(Nullable)' == '' ">enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
    <GraphLinqQLCodeGenerationAssemblyPath>$(MSBuildThisFileDirectory)GraphLinqQL.CodeGeneration\bin\Debug\net45\GraphLinqQL.CodeGeneration.exe</GraphLinqQLCodeGenerationAssemblyPath>
  </PropertyGroup>
  <!-- <Import Project="build\common.props" /> -->

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="2.9.5-beta1.final">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>
</Project>